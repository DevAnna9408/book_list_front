{
  "name": "front-framework",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build",
    "testbuild": "vue-cli-service build --mode awstest",
    "productBuild": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/assets/lang/**/*.json'",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apexsoft-quill-image-resize-module": "^3.0.1",
    "aws-sdk": "^2.406.0",
    "axios": "^0.18.0",
    "css-loader": "^3.4.0",
    "install": "^0.12.2",
    "jquery": "^3.4.1",
    "moment-timezone": "^0.5.28",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "quill": "^1.3.6",
    "quill-image-drop-module": "^1.0.3",
    "quill-video-resize-module2": "0.0.1",
    "raw-loader": "^4.0.2",
    "sweetalert2": "^9.4.1",
    "vee-validate": "3.4.5",
    "verte": "^0.0.12",
    "vue": "^2.6.9",
    "vue-carousel": "0.17.0",
    "vue-clickaway": "2.2.2",
    "vue-focus": "^2.1.0",
    "vue-google-adsense": "^1.10.1",
    "vue-i18n": "^8.0.0",
    "vue-kindergarten": "^0.3.3",
    "vue-lazyload": "1.2.6",
    "vue-moment": "^4.0.0",
    "vue-plugin-load-script": "^1.2.0",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.1.2",
    "vue-script2": "^2.1.0",
    "vue-select": "^3.11.2",
    "vue-slick": "^1.1.15",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.1.2",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-mocha": "^3.4.0",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "cypress": "^5.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "js-cookie": "^2.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.1.1",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
